        "vek280": {
            "param_set": [  "test_0_fft_ifft_dit_1ch_aie2",
                            "test_1_fft_ifft_dit_1ch_aie2",
                            "test_2_fft_ifft_dit_1ch_aie2",
                            "test_3_fft_ifft_dit_1ch_aie2",
                            "test_4_fft_ifft_dit_1ch_aie2",
                            "test_5_fft_ifft_dit_1ch_aie2",
