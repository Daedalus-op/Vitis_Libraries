      "default": "cint16",
      "validator": {
        "file": "vmc_fft_common.py",
        "function": "vmc_validate_twiddle_type",
        "args": ["data_type"]
      },
      "blockgui":{
            "label": "Twiddle factor data type:",
            "tablabel": "Main",
            "parameterType":"popup"
      }
    },
      {
      "key": "TP_POINT_SIZE",
      "name": "point_size",
      "description": "The number of samples in the transform",
      "default": 64,
      "validator": {
        "file": "vmc_fft_common.py",
