      "blockgui":{
          "label": "FFT size:",  
          "tablabel": "Main",
            "parameterType":"edit"
      }
    },
    {
      "key": "TP_WINDOW_VSIZE",
      "name": "input_window_size",
      "description": "The number of samples in the input window",
      "default": 64,
