{
    "name": "Cholesky Complex on AIE",
    "description": "Cholesky Complex on AIE",
    "flow": "versal",
    "platform_allowlist": [
        "vck190"
    ],
    "data": [
        "./data"
    ],
    "platform_blocklist": [
        "vek280"
    ],
    "platform_properties": {
        "vck190": {
            "v++": {
                "compiler": {
                    "clflags": []
                },
                "linker": {
                    "ldclflags": [
                        "--config PROJECT/system.cfg"
                    ]
                },
                "package": {
                    "clflags": [
                        "--package.defer_aie_run"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "name": "aiesim-launch",
            "target": "aiesim",
            "sim_options": "--input-dir=$(CUR_DIR) --output-dir=$(CUR_DIR)"
        },
        {
            "name": "x86sim-launch",
            "target": "x86sim",
            "sim_options": "--input-dir=$(CUR_DIR) --output-dir=$(CUR_DIR)"
        }
    ],
    "v++": {
        "compiler": {
            "includepaths": []
        }
    },
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include",
                "LIB_DIR/L2/include",
                "LIB_DIR/L1/src",
                "./",
            ]
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "PROJECT/aie/platform.cpp",
                    "aie_kernel_files": [
                        "LIB_DIR/L1/src/aie/cholesky_complex_decomposition.cpp"
                    ],
                    "workdir": "./Work",
                    "name": "libadf.a",
                    "config": "aie.cfg"
                }
            ]
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 40960,
                "max_time_min": {
                    "vitis_aie_sim": 480,
                    "vitis_aie_x86sim": 200,
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 200,
                    "vitis_hw_run": 30
                }
            }
        ],
        "targets": [
            "vitis_aie_sim",
            "vitis_aie_x86sim"
        ],
        "category": "canary"
    }
}
